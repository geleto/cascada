{
  "name": "nunjucks",
  "description": "A powerful templating engine with inheritance, asynchronous control, and more (jinja2 inspired)",
  "version": "3.2.4",
  "author": "James Long <longster@gmail.com>",
  "dependencies": {
    "@eslint/js": "^9.13.0",
    "a-sync-waterfall": "^1.0.1",
    "asap": "^2.0.6",
    "commander": "^12.1.0",
    "globals": "^15.11.0",
    "he": "^1.2.0",
    "istanbul-lib-coverage": "^3.2.2",
    "istanbul-lib-instrument": "^6.0.3",
    "istanbul-lib-report": "^3.0.1",
    "istanbul-reports": "^3.1.7",
    "tiny-chalk": "^2.0.0"
  },
  "browser": "./browser/nunjucks.js",
  "devDependencies": {
    "@babel/cli": "^7.25.7",
    "@babel/core": "^7.25.8",
    "@babel/plugin-transform-modules-commonjs": "^7.25.7",
    "@babel/preset-env": "^7.25.8",
    "@babel/register": "^7.25.7",
    "@istanbuljs/nyc-config-babel": "^3.0.0",
    "babel-loader": "^9.2.1",
    "babel-plugin-istanbul": "^7.0.0",
    "babel-plugin-module-resolver": "5.0.2",
    "connect": "^3.7.0",
    "core-js": "^3.38.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.13.0",
    "expect.js": "*",
    "express": "4.x",
    "fs-extra": "^11.2.0",
    "get-port": "^5.1.1",
    "mocha": "<10.x",
    "module-alias": "^2.2.3",
    "node-libs-browser": "^2.2.1",
    "nyc": "^17.1.0",
    "playwright": "^1.48.1",
    "serve-static": "^1.16.2",
    "supertest": "*",
    "terser-webpack-plugin": "^5.3.10",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "buildDependencies": {
    "@babel/cli": "^7.0.0-beta.38",
    "@babel/core": "^7.0.0-beta.38",
    "@babel/preset-env": "^7.0.0-beta.38",
    "@babel/register": "^7.0.0-beta.38",
    "babel-loader": "^8.0.0-beta.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-module-resolver": "3.0.0-beta.5",
    "core-js": "^2.5.3",
    "module-alias": "^2.0.3",
    "node-libs-browser": "^0.7.0",
    "uglify-js": "^2.8.29",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.10.0"
  },
  "peerDependencies": {
    "chokidar": "^3.3.0"
  },
  "peerDependenciesMeta": {
    "chokidar": {
      "optional": true
    }
  },
  "_moduleAliases": {
    "babel-register": "@babel/register"
  },
  "engines": {
    "node": ">= 6.9.0"
  },
  "scripts": {
    "postinstall": "npx playwright install chromium --with-deps",
    "codecov": "codecov",
    "mocha": "cross-env NODE_ENV=test NODE_PATH=./tests/test-node-pkgs mocha --check-leaks -R spec tests",
    "lint": "eslint nunjucks scripts tests",
    "prepare": "npm run build",
    "build:transpile": "babel nunjucks --out-dir .",
    "build:bundle": "node scripts/bundle.js",
    "build": "npm run lint && npm run build:transpile && npm run build:bundle",
    "test:prep": "npm run build && node scripts/runprecompile.js",
    "test:node-no-prep": "cross-env NODE_ENV=test NODE_PATH=./tests/test-node-pkgs nyc mocha --check-leaks --require @babel/register tests",
    "test:node": "npm run test:prep && npm run test:node-no-prep",
    "test:browser-no-prep": "cross-env NODE_ENV=test node scripts/bundle.js && node scripts/testrunner.js",
    "test:browser": "npm run test:prep && npm run test:browser-no-prep",
    "test:pasync": "cross-env NODE_ENV=test NODE_PATH=./tests/test-node-pkgs nyc --reporter=html --reporter=text mocha --check-leaks --require @babel/register tests/async.js",
    "test": "npm run test:prep && npm run test:node-no-prep && npm run test:browser-no-prep  -- fullTest"
  },
  "bin": {
    "nunjucks-precompile": "./bin/precompile"
  },
  "main": "index.js",
  "files": [
    "bin/**",
    "browser/**",
    "src/**"
  ],
  "nyc": {
    "extends": "@istanbuljs/nyc-config-babel",
    "all": true,
    "include": [
      "nunjucks/**/*.js"
    ],
    "reporter": [
      "text",
      "lcov"
    ],
    "sourceMap": true,
    "instrument": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mozilla/nunjucks.git"
  },
  "keywords": [
    "template",
    "templating"
  ],
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/mozilla/nunjucks/issues"
  }
}
